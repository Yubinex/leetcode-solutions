# Ignore files within csharp subfolders
/csharp/**/bin/
/csharp/**/obj/

# Ignore files within python subfolders
/python/**/__pycache__/

# Ignore C++ build files
/cpp/*/*.out
/cpp/*/*.o
/cpp/*/*.exe
/cpp/*/*.so
/cpp/*/*.dll

# Ignore C build files
/c/*/*.out
/c/*/*.o
/c/*/*.exe
/c/*/*.so
/c/*/*.dll

# Ignore Go build and binary files
/go/*/*.exe
/go/*/*.exe~
/go/*/*.dll
/go/*/*.so
/go/*/*.o
/go/*/*.a

# Ignore Rust build files
/rust/*/target/
/rust/*/Cargo.lock

# Ignore Visual Studio Code files
.vscode
